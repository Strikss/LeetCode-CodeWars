 let isPalindrome=(x)=> {
    let xStr =x.toString();
    let xArr =xStr.split("");
    let newArr =[];
    for(let i=xArr.length-1;i>=0;i--){
    newArr.push(xArr[i]);
    }
    newArr=newArr.join('')
    
    if (x==parseInt(newArr)){
        return true
    }
    else{
        return false
    }
};
console.log(isPalindrome(121))
